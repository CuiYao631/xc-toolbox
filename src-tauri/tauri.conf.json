{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "小崔工具箱",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },

    "security": {
      "csp": null
    },
    "systemTray":{
      "iconPath": "icons/32x32.png"
    },
    "windows": [
      {
        "label": "launch",
        "alwaysOnTop":true, 
        "fullscreen": false,
        "resizable": true,
        "title": "tauri-launch",
        "width": 640,
        "height": 128,
        "transparent":true,
        "decorations": false,
        "center": true,
        "visible": false,
        "skipTaskbar": false
      }
    ],
    "macOSPrivateApi": true
  }
}
